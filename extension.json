{
  "displayName": "Google Ads",
  "name": "google-adwords",
  "platform": "web",
  "version": "1.0.0",
  "description": "Tracks conversions from users that click on your Google Ads.",
  "author": {
    "name": "Celeste Robinson"
  },
  "iconPath": "ads-logo.svg",
  "viewBasePath": "src/view/",
  "actions": [
    {
      "displayName": "Track Conversion",
      "name": "track-conversion",
      "schema": {
        "$schema": "http://json-schema.org/draft-04/schema#",
        "type": "object",
        "required": ["conversionId", "conversionLabel"],
        "properties": {
          "conversionId": {
            "type": "string",
            "minLength": 1
          },
          "conversionLabel": {
            "type": "string",
            "minLength": 1
          },
          "conversionValue": {
            "type": "string"
          },
          "currencyCode": {
            "type": "string",
            "minLength": 1
          },
          "orderId": {
            "type": "string"
          }
        }
      },
      "libPath": "src/lib/actions/trackConversion.js",
      "viewPath": "actions/trackConversion.html"
    }
  ]
}